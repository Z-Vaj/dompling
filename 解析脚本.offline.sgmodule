[URL Rewrite]
#^https:\/\/(.*)\/(.*\.js)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=surge-module&target=surge-module&del=true
^https:\/\/(.*)\/(.*\.js).*(sg=true)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=surge-module&target=surge-module&del=true
^https:\/\/(.*)\/(.*\.js).*(stash=true)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=qx-rewrite&target=stash-stoverride&del=true
^https:\/\/(.*)\/(.*\.js).*(loon=true)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=loon-plugin&target=loon-plugin&del=true