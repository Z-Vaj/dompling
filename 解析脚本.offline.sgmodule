#!name = 🔴 解析脚本
#!category=通用重写
#!desc=依赖于 https://script.hub

[URL Rewrite]
^https:\/\/(raw.*)\/(.*\.js)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=surge-module&target=surge-module&del=true
^https:\/\/(raw.*)\/(.*\.js).*(sg)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=surge-module&target=surge-module&del=true
^https:\/\/(raw.*)\/(.*\.js).*(stash)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=qx-rewrite&target=stash-stoverride&del=true
^https:\/\/(raw.*)\/(.*\.js).*(loon)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=loon-plugin&target=loon-plugin&del=true


^https:\/\/(raw.*)\/(.*\.snippet)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=qx-rewrite&target=surge-module&del=true
^https:\/\/(raw.*)\/(.*\.snippet).*(sg)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=qx-rewrite&target=surge-module&del=true
^https:\/\/(raw.*)\/(.*\.snippet).*(stash)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=qx-rewrite&target=stash-stoverride&del=true
^https:\/\/(raw.*)\/(.*\.snippet).*(loon)$ http://script.hub/file/_start_/https://$1/$2/_end_/$2.sgmodule?type=qx-rewrite&target=loon-plugin&del=true